{
  "name": "dendriform",
  "version": "1.0.0",
  "description": "A web based offline first outliner with sync",
  "repository": {
    "type": "git",
    "url": "https://github.com/aggregat4/dendriform"
  },
  "engines": {
    "node": ">=13.0.0",
    "npm": ">=6.13.0"
  },
  "main": "main.js",
  "scripts": {
    "clean": "rm -rf dist && mkdir dist",
    "compile": "npx tsc --noEmit",
    "eslint": "npx eslint src",
    "test": "npx ts-node test/runtests.ts",
    "bundle-esb": "npx esbuild --bundle --sourcemap --target=es2017 --outdir=dist --format=esm --minify --splitting src/ts/tree.ts src/example/main.ts && cp src/css/*.css dist/example && cp src/example/index2.html dist/example/index.html",
    "bt": "npm run compile && npm run bundle-esb && npm run test",
    "cbt": "npm run clean && npm run bundle-esb && npm run test",
    "watch": "npx chokidar 'src/**/*.*' --initial -c 'npm run bt'",
    "sloc": "npx sloc src",
    "unused-exports": "npx ts-unused-exports tsconfig.json",
    "serve": "npx http-server dist",
    "unused-dependencies": "npx depcheck"
  },
  "author": "Boris Terzic <boris@terzic.be>",
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/file-saver": "2.0.2",
    "@typescript-eslint/eslint-plugin": "4.26.1",
    "@typescript-eslint/parser": "4.26.1",
    "barecolor": "1.0.1",
    "ceylon": "0.9.1",
    "chokidar-cli": "2.1.0",
    "esbuild": "0.12.8",
    "eslint": "7.28.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-jsdoc": "35.1.3",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-prettier": "3.4.0",
    "jsdom": "16.6.0",
    "jsdom-global": "3.0.2",
    "lodash": "4.17.21",
    "prettier": "2.3.1",
    "ts-node": "10.0.0",
    "ts-unused-exports": "7.0.3",
    "typescript": "4.3.2"
  },
  "dependencies": {
    "file-saver": "2.0.5",
    "idb": "6.1.2",
    "lit-html": "1.4.1"
  }
}
