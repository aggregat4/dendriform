{
  "name": "dendriform",
  "version": "1.0.0",
  "description": "A web based outliner with offline capabilities",
  "main": "main.js",
  "scripts": {
    "build": "cp src/*.css dist && cp src/*.html dist && ./node_modules/.bin/webpack src/main.js dist/main.js",
    "serve": "http-server -p 9090 dist/",
    "watch": "onchange src/* -- npm run build -s",
    "livereload": "live-reload --port 9091 dist/"
  },
  "author": "Boris Terzic <boris@terzic.be>",
  "license": "Apache-2.0",
  "devDependencies": {
    "eslint": "^4.3.0",
    "eslint-plugin-import": "^2.7.0",
    "http-server": "^0.9.0",
    "live-reload": "^1.1.0",
    "onchange": "^3.2.1",
    "parallelshell": "^3.0.1",
    "standard": "^10.0.2",
    "webpack": "^2.7.0"
  },
  "dependencies": {
    "maquette": "^2.5.2",
    "pouchdb-browser": "^6.3.4"
  }
}
