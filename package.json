{
  "name": "dendriform",
  "version": "1.0.0",
  "description": "A web based offline outliner with sync",
  "repository": {
    "type": "git",
    "url": "https://github.com/aggregat4/dendriform"
  },
  "main": "main.js",
  "scripts": {
    "build": "cp src/css/*.css dist && cp src/example/*.css dist && cp src/example/*.html dist && ./node_modules/.bin/webpack",
    "serve": "http-server -p 9090 dist/",
    "watch": "onchange './src/**/*.*' './*.*' -i -- npm run build -s",
    "livereload": "live-reload --port 9091 dist/",
    "sloc": "node_modules/.bin/sloc src",
    "test": "jest",
    "test-watch": "npx jest --watch"
  },
  "author": "Boris Terzic <boris@terzic.be>",
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/jest": "^23.3.2",
    "http-server": "^0.11.0",
    "jest": "^23.6.0",
    "live-reload": "^1.1.0",
    "onchange": "^4.1.0",
    "parallelshell": "^3.0.2",
    "sloc": "^0.2.0",
    "ts-jest": "^23.1.0",
    "ts-loader": "^5.1.0",
    "tslint": "^5.11.0",
    "typescript": "^3.0.0",
    "webpack": "^4.18.0",
    "webpack-bundle-analyzer": "^3.0.0",
    "webpack-cli": "^3.1.0"
  },
  "dependencies": {
    "dexie": "^2.0.4",
    "p-queue": "^3.0.0",
    "redom": "^3.13.1"
  }
}
