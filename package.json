{
  "name": "dendriform",
  "version": "1.0.0",
  "description": "A web based offline first outliner with sync",
  "repository": {
    "type": "git",
    "url": "https://github.com/aggregat4/dendriform"
  },
  "engines": {
    "node": ">=13.0.0",
    "npm": ">=6.13.0"
  },
  "main": "main.js",
  "scripts": {
    "clean": "rm -rf dist && mkdir dist",
    "compile": "npx tsc --noEmit",
    "eslint": "npx eslint src",
    "test": "npx ts-node test/runtests.ts",
    "bundle-rollup": "npx rollup -c rollup.config.js",
    "bundle-esb": "npx esbuild --bundle --sourcemap --target=es2017 --outdir=dist --format=esm --minify --splitting src/ts/tree.ts src/example/main.ts && cp src/css/*.css dist/example && cp src/example/index2.html dist/example/index.html",
    "bt-rollup": "npm run bundle-rollup && npm run test",
    "cbt-rollup": "npm run clean && npm run bundle-rollup && npm run test",
    "bt": "npm run compile && npm run bundle-esb && npm run test",
    "cbt": "npm run clean && npm run bundle-esb && npm run test",
    "watch": "npx chokidar 'src/**/*.*' --initial -c 'npm run bt'",
    "sloc": "npx sloc src",
    "unused-exports": "npx ts-unused-exports tsconfig.json",
    "serve": "npx http-server dist",
    "unused-dependencies": "npx depcheck"
  },
  "author": "Boris Terzic <boris@terzic.be>",
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/file-saver": "2.0.1",
    "@typescript-eslint/eslint-plugin": "4.4.1",
    "@typescript-eslint/parser": "4.4.1",
    "@wessberg/rollup-plugin-ts": "1.3.5",
    "barecolor": "1.0.1",
    "ceylon": "0.9.1",
    "chokidar-cli": "2.1.0",
    "esbuild": "0.7.15",
    "eslint": "7.11.0",
    "eslint-config-prettier": "6.12.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "30.6.5",
    "eslint-plugin-prefer-arrow": "1.2.2",
    "eslint-plugin-prettier": "3.1.4",
    "jsdom": "16.4.0",
    "jsdom-global": "3.0.2",
    "lodash": "4.17.20",
    "prettier": "2.1.2",
    "rollup": "2.28.2",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-copy": "3.3.0",
    "rollup-plugin-eslint": "7.0.0",
    "rollup-plugin-html2": "2.0.0",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-visualizer": "4.1.1",
    "ts-node": "9.0.0",
    "ts-unused-exports": "6.2.4",
    "typescript": "4.0.3"
  },
  "dependencies": {
    "file-saver": "2.0.2",
    "idb": "5.0.7",
    "lit-html": "1.3.0"
  }
}
