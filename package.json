{
  "name": "dendriform",
  "version": "1.0.0",
  "description": "A web based outliner with offline capabilities",
  "repository": {
    "type": "git",
    "url": "https://github.com/aggregat4/dendriform"
  },
  "main": "main.js",
  "scripts": {
    "build": "cp src/*.css dist && cp src/*.html dist && ./node_modules/.bin/webpack",
    "serve": "http-server -p 9090 dist/",
    "watch": "onchange src/* -- npm run build -s",
    "livereload": "live-reload --port 9091 dist/"
  },
  "author": "Boris Terzic <boris@terzic.be>",
  "license": "Apache-2.0",
  "devDependencies": {
    "http-server": "^0.10.0",
    "live-reload": "^1.1.0",
    "onchange": "^3.2.1",
    "parallelshell": "^3.0.2",
    "ts-loader": "^3.1.1",
    "tslint": "^5.8.0",
    "typescript": "^2.6.1",
    "webpack": "^3.8.1"
  },
  "dependencies": {
    "maquette": "^3.0.1",
    "p-queue": "^2.3.1",
    "pouchdb-browser": "^6.3.4",
    "redom": "^3.11.1"
  }
}
