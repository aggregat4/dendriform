{
  "name": "dendriform",
  "version": "1.0.0",
  "description": "A web based offline first outliner with sync",
  "repository": {
    "type": "git",
    "url": "https://github.com/aggregat4/dendriform"
  },
  "engines": {
    "node": ">=13.0.0",
    "npm": ">=6.13.0"
  },
  "main": "main.js",
  "scripts": {
    "copycss": "cp src/css/tree.css dist",
    "clean": "rm -rf dist && mkdir dist",
    "build": "npx rollup -c rollup.config.js",
    "test": "npx ts-node test/runtests.ts",
    "bt": "npm run build && npm run test",
    "cbt": "npm run clean && npm run build && npm run test",
    "watch": "npx rollup -c rollup.config.js -w",
    "sloc": "npx sloc src",
    "unused-exports": "npx ts-unused-exports tsconfig.json",
    "serve": "npx http-server dist",
    "unused-dependencies": "npx depcheck"
  },
  "author": "Boris Terzic <boris@terzic.be>",
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/file-saver": "2.0.1",
    "@typescript-eslint/eslint-plugin": "^3.4.0",
    "@typescript-eslint/parser": "^3.4.0",
    "@wessberg/rollup-plugin-ts": "1.2.26",
    "barecolor": "1.0.1",
    "ceylon": "0.9.1",
    "eslint": "^7.3.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jsdoc": "^28.5.1",
    "eslint-plugin-prefer-arrow": "^1.2.1",
    "jsdom": "16.2.2",
    "jsdom-global": "3.0.2",
    "rollup": "2.18.0",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-html2": "1.1.0",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-visualizer": "4.0.4",
    "ts-unused-exports": "6.2.1",
    "tslint": "6.1.2",
    "typescript": "3.9.5"
  },
  "dependencies": {
    "file-saver": "2.0.2",
    "idb": "5.0.4",
    "lit-html": "1.2.1"
  }
}
